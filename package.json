    "uuid": "^13.0.0",
    "vue": "^3.2.13",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
